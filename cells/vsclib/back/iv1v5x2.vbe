ENTITY iv1v5x2 IS
GENERIC (
  CONSTANT area          : NATURAL := 1728;   
  CONSTANT cin_a         : NATURAL := 5;      
  CONSTANT rdown_a_z     : NATURAL := 2080;   
  CONSTANT rup_a_z       : NATURAL := 2120;   
  CONSTANT tphl_a_z      : NATURAL := 36;     
  CONSTANT tplh_a_z      : NATURAL := 37;     
  CONSTANT transistors   : NATURAL := 2       
);
PORT (
  a	 : in  BIT;
  z	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END iv1v5x2;

ARCHITECTURE behaviour_data_flow OF iv1v5x2 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on iv1v5x2"
  SEVERITY WARNING;
  z <= not (a) after 89 ps;
END;
